ggplot_histo_pareto <- function (data, bin_width) {
# Cumulative data uses same bins as histogram
rounding_factor <- 1 / bin_width
data_rows <- nrow(data)
data_rounded <- select(data) %>%
mutate(data_rounded = round(data * rounding_factor) / rounding_factor) %>%
mutate(cumulative_rows = row_number() / data_rows)
data_mean <- mean(data)
data_median <- median(data)
x_max <- max(data)
y_max <- data_rows
p <- ggplot() +
geom_histogram(x = data, binwidth = bin_width, col="white") +
scale_x_continuous(breaks = seq(0, xmax, by = bin_width)) +
geom_line(aes(x = data_rounded$data_rounded,
y = data_rounded$cumulative_users * ymax),
col="red") +
geom_vline(aes(xintercept = data_mean, col = 'red')) +
geom_vline(aes(xintercept = data_median), col = 'blue') +
scale_y_continuous(name = "Count",
breaks = seq(0, ymax, by = 1),
sec.axis = sec_axis(~./ymax,name = "Cumulative Percentage of Count"))
return(p)
}
